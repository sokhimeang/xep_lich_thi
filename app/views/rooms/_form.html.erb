<%= form_for @room do |f| %>
  <%= render "shared/error_messages", object: f.object %>

  <%= f.label :code %>
  <%= f.text_field :code, class: "form-control" %>

  <%= f.label :capacity %>
  <%= f.text_field :capacity, class: "form-control" %>

  <br><%= f.submit yield(:btn_form), class: "btn btn-primary" %>
<% end %>
